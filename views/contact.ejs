      /* Updated styles for Contact Information section */

      .contact-info-section {
        background-color: #007bff;

        background-image: linear-gradient(to right, green, lime);

        padding: 40px 0;

        text-align: center;

        color: #fff;
      }

      .contact-info-heading {
        font-size: 36px;

        font-weight: bold;

        margin-bottom: 30px;
      }

      .contact-info-text {
        font-size: 18px;

        line-height: 1.6;

        max-width: 800px;

        margin: 0 auto;
      }

      .contact-details {
        margin-top: 30px;
      }

      .contact-details p {
        font-size: 18px;

        margin-bottom: 10px;
      }

      .contact-details a {
        color: #fff;

        text-decoration: none;
      }

      .contact-details a:hover {
        text-decoration: underline;
      }

      /* Updated styles for Contact Options section */

      .contact-options {
        margin-top: 30px;

        display: flex;

        justify-content: center;

        align-items: center;
      }

      .contact-option {
        flex: 1;

        padding: 20px;

        border: 5px solid #fff;

        border-radius: 5px;

        margin: 10px;

        transition: all 0.3s ease;

        cursor: pointer;

        background-color: #7f8484;

        color: #fff;

        text-align: center;
      }

      .contact-option:hover {
        background-color: limegreen
      }

      .contact-icon {
        font-size: 32px;

        margin-bottom: 10px;
      }
